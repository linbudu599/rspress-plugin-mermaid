{
  "name": "rspress-plugins",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./packages/**/{dist,build}",
    "build": "pnpm run clean && pnpm packages:build",
    "commit": "git add -A && czg",
    "packages:build": "pnpm -r --filter=./packages/**/* run build",
    "test": "vitest",
    "testu": "vitest --update",
    "test:cov": "vitest --coverage"
  },
  "config": {
    "commitizen": {
      "path": "cz-git"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.0",
    "@types/pacote": "^11.1.8",
    "@vitest/coverage-v8": "^1.4.0",
    "cac": "^6.7.14",
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0",
    "clsx": "^2.1.0",
    "consola": "^3.2.3",
    "czg": "^1.9.1",
    "eslint": "^9.0.0",
    "execa": "^8.0.1",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "stylelint": "^16.3.1",
    "typescript": "^5.4.4",
    "util-ts-types": "^1.0.0",
    "vitest": "1.4.0"
  },
  "dependencies": {}
}