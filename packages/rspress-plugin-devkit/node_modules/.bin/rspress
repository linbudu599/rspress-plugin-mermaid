#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/linbudu/Desktop/Infra/rspress-plugin-mermaid/node_modules/.pnpm/rspress@1.17.1_webpack@5.91.0/node_modules/rspress/bin/node_modules:/Users/linbudu/Desktop/Infra/rspress-plugin-mermaid/node_modules/.pnpm/rspress@1.17.1_webpack@5.91.0/node_modules/rspress/node_modules:/Users/linbudu/Desktop/Infra/rspress-plugin-mermaid/node_modules/.pnpm/rspress@1.17.1_webpack@5.91.0/node_modules:/Users/linbudu/Desktop/Infra/rspress-plugin-mermaid/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/linbudu/Desktop/Infra/rspress-plugin-mermaid/node_modules/.pnpm/rspress@1.17.1_webpack@5.91.0/node_modules/rspress/bin/node_modules:/Users/linbudu/Desktop/Infra/rspress-plugin-mermaid/node_modules/.pnpm/rspress@1.17.1_webpack@5.91.0/node_modules/rspress/node_modules:/Users/linbudu/Desktop/Infra/rspress-plugin-mermaid/node_modules/.pnpm/rspress@1.17.1_webpack@5.91.0/node_modules:/Users/linbudu/Desktop/Infra/rspress-plugin-mermaid/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rspress/bin/rspress.js" "$@"
else
  exec node  "$basedir/../rspress/bin/rspress.js" "$@"
fi
